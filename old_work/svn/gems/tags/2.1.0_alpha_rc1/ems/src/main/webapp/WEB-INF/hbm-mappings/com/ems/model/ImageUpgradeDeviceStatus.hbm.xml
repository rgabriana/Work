<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.ImageUpgradeDeviceStatus" table="image_upgrade_device_status">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">image_upgrade_device_status_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-ImageUpgradeDeviceStatus.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>

		<property name="jobId" type="java.lang.Long" update="true"
			insert="true" column="job_id" />

		<property name="startTime" type="java.util.Date" update="true"
			insert="true" column="start_time" />

		<property name="endTime" type="java.util.Date" update="true"
			insert="true" column="end_time" />

		<property name="noOfAttempts" type="java.lang.Integer"
			update="true" insert="true" column="no_of_attempts" />

		<property name="status" type="java.lang.String" update="true"
			insert="true" column="status" />

		<property name="description" type="java.lang.String" update="true"
			insert="true" column="description" />

		<property name="deviceId" type="java.lang.Long" update="true"
			insert="true" column="device_id" />

		<!-- To add non XDoclet property mappings, create a file named hibernate-properties-ImageUpgradeDeviceStatus.xml 
			containing the additional properties and place it in your merge dir. -->

	</class>

</hibernate-mapping>
