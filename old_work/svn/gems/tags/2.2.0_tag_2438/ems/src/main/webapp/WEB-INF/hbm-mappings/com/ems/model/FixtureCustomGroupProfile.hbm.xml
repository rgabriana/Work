<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.FixtureCustomGroupsProfile" table="custom_fixture_profile_group">


		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">custom_fixture_profile_group_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-Fixture.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>
		
		<property
            name="fixtureId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="fixture_id"
        />
		
		<property
            name="profileHandler"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="profile_handler_id"
        />
        <property
            name="groupId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="group_id"
        />
	</class>
</hibernate-mapping>
