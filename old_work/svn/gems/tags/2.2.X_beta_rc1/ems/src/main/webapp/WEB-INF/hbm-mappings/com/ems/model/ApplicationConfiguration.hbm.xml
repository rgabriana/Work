<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.ApplicationConfiguration"
        table="application_configuration"
        mutable="false"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">application_configuration_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ApplicationConfiguration.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="selfLogin"
            type="boolean"
            update="true"
            insert="true"
            column="self_login"
        />

        <property
            name="validDomain"
            type="java.lang.String"
            update="true"
            insert="true"
            column="valid_domain"
        />

        <property
            name="price"
            type="java.lang.Float"
            update="true"
            insert="true"
            column="price"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ApplicationConfiguration.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
