<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.PlugloadProfile"
        table="plugload_profile"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">plugload_profile_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Profile.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="activeMotion"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="active_motion_window"
        />

        <property
            name="mode"
            type="java.lang.Byte"
            update="true"
            insert="true"
            column="mode"
        />
	   <property
            name="manualOverrideTime"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="manual_override_time"
        />

       

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Profile.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
