<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.BacnetReportConfiguration" table="bacnet_report_cfg">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">bacnet_report_cfg_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-BACnetConfiguration.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>
		
		<property name="deviceid" type="java.lang.String" update="true"
			insert="true" column="deviceid" />
		
		<property name="objecttype" type="java.lang.String" update="true"
			insert="true" column="objecttype" />
			
		<property name="objectinstance" type="java.lang.String" update="true"
			insert="true" column="objectinstance" />
		
		<property name="objectname" type="java.lang.String" update="true"
			insert="true" column="objectname" />

		<!-- To add non XDoclet property mappings, create a file named hibernate-properties-BACnetConfiguration.xml 
			containing the additional properties and place it in your merge dir. -->

	</class>

</hibernate-mapping>
