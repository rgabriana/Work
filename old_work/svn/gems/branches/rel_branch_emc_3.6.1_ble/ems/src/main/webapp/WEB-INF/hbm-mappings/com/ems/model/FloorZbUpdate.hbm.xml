<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.FloorZbUpdate"
        table="floor_zbupdate"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">floor_zbupdate_seq</param>
            </generator>
        </id>
		
		  <property
            name="floorId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="floor_id"
        />
		
         
		
		  <property
            name="startTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="start_time"
        />
         <property
            name="endTime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="end_time"
        />
        
        <property
            name="processedState"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="processed_state"
        />

    </class>

</hibernate-mapping>
