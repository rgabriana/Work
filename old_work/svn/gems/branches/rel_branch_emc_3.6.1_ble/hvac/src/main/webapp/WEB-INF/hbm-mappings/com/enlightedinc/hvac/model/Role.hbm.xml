<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.enlightedinc.hvac.model.Role" table="roles" mutable="false">

		<cache usage="read-only" />
		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">role_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-Role.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>
		
		<property name="name" type="java.lang.String" update="true"
			insert="true" column="name" />


	</class>

</hibernate-mapping>
