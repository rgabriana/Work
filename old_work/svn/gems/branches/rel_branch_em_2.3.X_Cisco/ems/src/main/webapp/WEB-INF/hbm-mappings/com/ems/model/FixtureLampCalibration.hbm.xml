<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.FixtureLampCalibration" table="fixture_lamp_calibration">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">fixture_lamp_calibration_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-Area.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>

		<property name="captureAt" type="java.util.Date" update="true"
			insert="true" column="capture_at" />
		<property name="fixtureId" type="java.lang.Long" update="true"
			insert="true" column="fixture_id" />
		<property name="initial" type="java.lang.Boolean" update="true"
			insert="true" column="initial" />
		<set name="fixtureCalibrationMap" inverse="true" lazy="false"
			cascade="delete" sort="unsorted" outer-join="false">

			<key column="fixture_lamp_calibration_id">
			</key>

			<one-to-many class="com.ems.model.FixtureCalibrationMap" />
		</set>
	</class>
</hibernate-mapping>