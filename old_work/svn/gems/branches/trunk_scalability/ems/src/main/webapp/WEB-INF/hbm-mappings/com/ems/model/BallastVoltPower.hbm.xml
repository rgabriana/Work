<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.BallastVoltPower" table="ballast_volt_power" >

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">ballast_volt_power_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-Fixture.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>

		<property name="ballastId" type="java.lang.Long" update="true"
			insert="true" column="ballast_id" />

		<property name="voltPowerMapId" type="java.lang.Long" update="true"
			insert="true" column="volt_power_map_id" />

		<property name="volt" type="java.lang.Double" update="true"
			insert="true" column="volt" />

		<property name="power" type="java.lang.Double" update="true"
			insert="true" column="power" />

		<property name="inputVolt" type="java.lang.Double" update="true"
			insert="true" column="inputvolt" />
			
		<property name="enabled" type="java.lang.Boolean" update="true"
			insert="true" column="enabled" />

		<!-- To add non XDoclet property mappings, create a file named hibernate-properties-BallastVoltPower.xml 
			containing the additional properties and place it in your merge dir. -->

	</class>

</hibernate-mapping>
