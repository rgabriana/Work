<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.FixtureDistances"
        table="fixturedistances"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">fixturedistances_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-FixtureDistances.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="srcFixture"
            type="java.lang.String"
            update="true"
            insert="true"
            column="[src_fixture]"
        />

        <property
            name="dstFixture"
            type="java.lang.String"
            update="true"
            insert="true"
            column="[dst_fixture]"
        />

        <property
            name="lightLevel"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="[light_level]"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-FixtureDistances.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
