<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.occengine.model.SensorRGL" table="sensor_rgl">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">sensor_rgl_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-Fixture.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>
		
		<property name="mac" type="java.lang.String" update="true"
			insert="true" column="mac" />

		<many-to-one name="zone" class="com.occengine.model.Zone"
			cascade="none" outer-join="auto" update="true" insert="true" column="zone_id" />
				
	</class>

</hibernate-mapping>
