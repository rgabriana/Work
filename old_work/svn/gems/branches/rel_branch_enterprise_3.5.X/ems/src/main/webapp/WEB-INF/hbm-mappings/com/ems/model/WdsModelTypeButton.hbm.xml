<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.WdsModelTypeButton"
        table="wds_model_type_button"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">wds_model_type_button_seq</param>
            </generator>
        </id>

        <property
            name="wdsModelTypeId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="wds_model_type_id"
        />
        
        <property
            name="buttonNo"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="button_no"
        />
        
        <property
            name="buttonName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="button_name"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PlanMap.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
