<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.BacnetObjectsCfg" table="bacnet_objects_cfg">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">bacnet_objects_cfg_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-BACnetConfiguration.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>
		
		<property name="bacnetobjecttype" type="java.lang.String" update="true"
			insert="true" column="bacnetobjecttype" />
		
		<property name="bacnetobjectinstance" type="java.lang.Long" update="true"
			insert="true" column="bacnetobjectinstance" />

		<property name="bacnetobjectdescription" type="java.lang.String" update="true"
			insert="true" column="bacnetobjectdescription" />
			
		<property name="isvalidobject" type="java.lang.String" update="true"
			insert="true" column="isvalidobject" />
		
		<property name="pointkeyword" type="java.lang.String" update="true"
			insert="true" column="pointkeyword" />

		<property name="bacnetpointtype" type="java.lang.String" update="true"
			insert="true" column="bacnetpointtype" />

		<!-- To add non XDoclet property mappings, create a file named hibernate-properties-BACnetConfiguration.xml 
			containing the additional properties and place it in your merge dir. -->

	</class>

</hibernate-mapping>
