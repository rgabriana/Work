<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.GemsGroupPlugload" table="gems_group_plugload">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">gems_group_plugload_seq</param>
			</generator>
		</id>

		<many-to-one name="group" class="com.ems.model.GemsGroup"
			cascade="none" outer-join="auto" update="true" insert="true" column="group_id" />

		<many-to-one name="plugload" class="com.ems.model.Plugload"
			cascade="none" outer-join="auto" update="true" insert="true" column="plugload_id" />

		<property name="needSync" type="java.lang.Long" update="true"
			insert="true" column="need_sync" />

		<property name="userAction" type="java.lang.Long" update="true"
			insert="true" column="user_action" />
		
		<one-to-one name="motionGrpPlDetails" property-ref="gemsGroupPlugload" cascade="all"></one-to-one>

	</class>

</hibernate-mapping>
