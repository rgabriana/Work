<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.CannedProfileConfiguration"
        table="canned_profiles_configuration"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">cannedprofile_configuration_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SystemConfiguration.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />        
        
        <property
            name="parentProfileid"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="parentprofileid"
        />
          
          <property
            name="status"
            type="java.lang.Boolean"
            update="true"
            insert="true"
            column="status"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SystemConfiguration.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
