<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.OutageBasePower"
        table="outage_base_power"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">outage_base_power_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-OutageBasePower.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="fixtureId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="fixture_id"
        />

        <property
            name="voltLevel"
            type="java.lang.Short"
            update="true"
            insert="true"
            column="volt_level"
        />

        <property
            name="basePower"
            type="java.lang.Double"
            update="true"
            insert="true"
            column="base_power"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-OutageBasePower.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
