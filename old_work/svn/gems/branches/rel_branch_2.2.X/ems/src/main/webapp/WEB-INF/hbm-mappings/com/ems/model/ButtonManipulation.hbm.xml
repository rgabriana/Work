<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.ButtonManipulation"
        table="button_manipulation"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">button_manipulation_seq</param>
            </generator>
        </id>

        <property
            name="buttonMapId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="button_map_id"
        />
        
        <property
            name="sceneToggleOrder"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="scene_toggle_order"
        />
        
        <property
            name="buttonManipAction"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="button_manip_action"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PlanMap.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
