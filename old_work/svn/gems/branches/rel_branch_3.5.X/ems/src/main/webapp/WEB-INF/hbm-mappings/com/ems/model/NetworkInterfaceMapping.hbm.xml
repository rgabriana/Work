<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.NetworkInterfaceMapping" table="network_interface_mapping">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">network_interface_mapping_seq</param>
			</generator>
		</id>


		<many-to-one name="networkSettings" class="com.ems.model.NetworkSettings"
			cascade="save-update" outer-join="auto" update="true" insert="true"
			column="network_settings_id" />
		<many-to-one name="networkTypes" class="com.ems.model.NetworkTypes"
			cascade="save-update" outer-join="auto" update="true" insert="true"
			column="network_type_id" />
	</class>

</hibernate-mapping>
