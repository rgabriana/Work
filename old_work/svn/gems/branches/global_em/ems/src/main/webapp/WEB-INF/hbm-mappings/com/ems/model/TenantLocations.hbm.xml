<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ems.model.TenantLocations" table="tenant_locations">

		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native">
				<param name="sequence">tenant_locations_seq</param>
				<!-- To add non XDoclet generator parameters, create a file named hibernate-generator-params-User.xml 
					containing the additional parameters and place it in your merge dir. -->
			</generator>
		</id>

		<property name="approvedLocationType" update="true" insert="true"
			column="approved_location_type">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.ems.types.FacilityType</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="locationId" type="java.lang.Long" update="true"
			insert="true" column="location_id" />
		<many-to-one name="tenant" class="com.ems.model.Tenant"
			cascade="none" outer-join="auto" update="true" insert="true" column="tenant_id" />


		<!-- To add non XDoclet property mappings, create a file named hibernate-properties-User.xml 
			containing the additional properties and place it in your merge dir. -->

	</class>

</hibernate-mapping>