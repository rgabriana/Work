<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
	"http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
	<tlibversion>1.0.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>CustomTag</shortname>
	<uri>http://ecloud.enlightedinc/tags</uri>
	<info>ecloud tags</info>
	<tag>
		<name>showAppVersion</name>
		<tagclass>com.emscloud.tags.AppVersionTag</tagclass>
		<bodycontent>empty</bodycontent>
		<info>Tag to display current version</info>
	</tag>
	<tag>
		<name>showFacilityTree</name>
		<tagclass>com.emscloud.tags.FacilityTreeTag</tagclass>
		<bodycontent>empty</bodycontent>
		<info>Tag to create Facility Tree from facilityTreeHierarchy TreeNode Object</info>
		<attribute>
			<name>facilityTreeHierarchy</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>showProfileTree</name>
		<tagclass>com.emscloud.tags.ProfileTreeTag</tagclass>
		<bodycontent>empty</bodycontent>
		<info>Tag to create Profile Tree from profileTreeHierarchy TreeNode Object</info>
		<attribute>
			<name>profileTreeHierarchy</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>showUEMProfileTree</name>
		<tagclass>com.emscloud.tags.UEMProfileTreeTag</tagclass>
		<bodycontent>empty</bodycontent>
		<info>Tag to create UEM Profile Tree from profileTreeHierarchy TreeNode Object</info>
		<attribute>
			<name>uemProfileTreeHierarchy</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>breakDateDiffInString</name>
		<tagclass>com.emscloud.tags.DateIntervalTag</tagclass>
		<bodycontent>empty</bodycontent>
		<info>Tag to break down difference between the specified date and current date into String of combination of hours, minutes and seconds</info>
		<attribute>
			<name>dateValue</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>datePattern</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>breakSecondsInString</name>
		<tagclass>com.emscloud.tags.SecondsIntervalStringTag</tagclass>
		<bodycontent>empty</bodycontent>
		<info>Tag to break down seconds interval into String of combination of hours, minutes and seconds</info>
		<attribute>
			<name>seconds</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>