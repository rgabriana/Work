<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.ems.model.WeekdayPlugload"
        table="weekday_plugload"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">weekday_plugload_seq</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-WeekDay.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="day"
            type="java.lang.String"
            update="true"
            insert="true"
            column="day"
        />

        <many-to-one
            name="plugloadProfileConfiguration"
            class="com.ems.model.PlugloadProfileConfiguration"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="plugload_profile_configuration_id"
        />

        <property
            name="shortOrder"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="short_order"
        />

        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            column="type"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-WeekDay.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
